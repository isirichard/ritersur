<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>Prueba1</servlet-name>
		<servlet-class>epis.Prueba1</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Prueba1</servlet-name>
		<url-pattern>/prueba1</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>Save</servlet-name>
		<servlet-class>epis.Save</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Save</servlet-name>
		<url-pattern>/save</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>LoginController</servlet-name>
		<servlet-class>epis.LoginController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LoginController</servlet-name>
		<url-pattern>/LoginController</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>SaveRiego</servlet-name>
		<servlet-class>epis.SaveRiego</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>SaveRiego</servlet-name>
		<url-pattern>/saveRiego</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>ActualizarLista</servlet-name>
		<servlet-class>epis.ActualizarLista</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ActualizarLista</servlet-name>
		<url-pattern>/actualizarLista</url-pattern>
	</servlet-mapping>
	
	<welcome-file-list>
		<welcome-file>loginUsuario.jsp</welcome-file>
	</welcome-file-list>
</web-app>
